class Employee {
    constructor(name, numQD, status, degree, 
        date_apply_from, date_apply_to, position, 
        level_from, level_to, coe_salary_from, coe_salary_to, date_next_rise_from, date_next_rise_to){
            this.name = name;
            this.numQD = numQD;
			this.status= status
            this.degree = degree;
            this.date_apply_from = date_apply_from;
            this.date_apply_to = date_apply_to;
            this.position = position;
            this.level_from = level_from;
            this.level_to = level_to;
            this.coe_salary_from = coe_salary_from;
            this.coe_salary_to = coe_salary_to;
			this.date_next_rise_from= date_next_rise_from
			this.date_next_rise_to= date_next_rise_to
    }
    toString() {
        return `Name: ${this.name}\nNumber QD: ${this.numQD}\nStatus: ${this.status}\nDegree: ${this.degree}\nDate Apply From: ${this.date_apply_from}\nDate Apply To: ${this.date_apply_to}\nPosition: ${this.position}\nLevel From: ${this.level_from}\nLevel To: ${this.level_to}\nCOE Salary From: ${this.coe_salary_from}\nCOE Salary To: ${this.coe_salary_to}\nDate Salary next risefrom: ${this.date_next_rise_from}\nDate Salary next rise To: ${this.date_next_rise_to}`;
      }
    }
