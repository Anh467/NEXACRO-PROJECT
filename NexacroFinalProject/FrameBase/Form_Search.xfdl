// get string search of each element
this.GetStrSearch= function (input, id, isStrSearchNotEmpty)
{
	var strSearch= ""
	if (input !== undefined && input !== "" ){
		if(isStrSearchNotEmpty) strSearch += " && "
			strSearch += "String("+id+").toLowerCase().trim().indexOf('"+input.toLowerCase().trim()+"')>=0";
	}
	return strSearch
};
this.GetStrSearchBetweenDate= function (from, to, id, isStrSearchNotEmpty)
{
	var strSearch= ""
	if ( (from !== undefined && from !== "") && (to !== undefined && to !== "") ){
		if(isStrSearchNotEmpty) strSearch += " && "
		strSearch += id+" >= '"+from +"' && "+ id+" <= '"+to+"'";
	}
	return strSearch
};
this.GetStrSearchBetweenFloat= function (from, to, id, isStrSearchNotEmpty)
{
	var strSearch= ""
	if ( (from !== undefined && from !== "") && (to !== undefined && to !== "") ){
		if(isStrSearchNotEmpty) strSearch += " && "
		strSearch += id+" >= "+from +" && "+ id+" <= "+to+"";
	}
	return strSearch
};
//search
this.SearchHandler= function (input)
{
	var strSearch =""
// 	const NUM_QD= "NUM_QD"
	strSearch += this.GetStrSearch(input.numQD, NUM_QD, strSearch.length > 0)
// * const DATE_APPLY= "DATE_APPLY"
	strSearch += this.GetStrSearchBetweenDate(input.date_apply_from, input.date_apply_to, DATE_APPLY, strSearch.length > 0)
// 	const NAME= "NAME"
	strSearch += this.GetStrSearch(input.name, NAME, strSearch.length > 0)
// 	const STATUS= "STATUS"
	strSearch += this.GetStrSearch(input.status, STATUS, strSearch.length > 0)
// 	const POSITION= "POSITION"
	strSearch += this.GetStrSearch(input.position, POSITION, strSearch.length > 0)
// 	const DEGREE= "DEGREE"
	strSearch += this.GetStrSearch(input.degree, DEGREE, strSearch.length > 0)
// * const LEVEL_APPLY= "LEVEL_APPLY"
	strSearch += this.GetStrSearchBetweenFloat(input.level_from, input.level_to, LEVEL_APPLY, strSearch.length > 0)
// * const COE_SALARY= "COE_SALARY"
 	strSearch += this.GetStrSearchBetweenFloat(input.coe_salary_from, input.coe_salary_to, COE_SALARY, strSearch.length > 0)
// const DATE_COE_SALARY_NEXT_RISE= "DATE_COE_SALARY_NEXT_RISE"
	strSearch += this.GetStrSearchBetweenDate(input.date_next_rise_from, input.date_next_rise_to, DATE_COE_SALARY_NEXT_RISE, strSearch.length > 0)
/*
	chưa có component trong form
	const ALLOWANCE_COE_SALARY= "ALLOWANCE_COE_SALARY"
 	const ALLOWANCE_POSITION= "ALLOWANCE_POSITION"
 	const ALLOWANCE_RESPONSIBILITY= "ALLOWANCE_RESPONSIBILITY"
 	const APPLING= "APPLING"
*/
	//trace("check strSearch: "+ strSearch)
	var ds_Track_Working=  nexacro.getApplication().ds_Track_Working
	ds_Track_Working.filter(strSearch)
	return ds_Track_Working.getRowCount()
};
