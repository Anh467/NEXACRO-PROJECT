this.DeletesHandlerFunction= function(){
	var strNumQD= ""
	var ds_Track_Working=  nexacro.getApplication().ds_Track_Working
	ds_Track_Working.filter("")
	var rowNF= ds_Track_Working.getRowCountNF()
	for( let i = 0; i < rowNF; i++){
		var Delete= ds_Track_Working.getColumn(i, DELETE)
		var numQD= ds_Track_Working.getColumn(i, NUM_QD)
		if(Delete == "true"){
			if( strNumQD.length > 0 )
				strNumQD += ", "
			strNumQD += numQD
			
		}	
	}
	if(strNumQD == ""){
		alert("!!!Bạn phải chọn số quy định để có thể sử dụng chức năng xóa!!!")
