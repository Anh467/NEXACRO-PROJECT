				obj.setCellProperty( "head", i, "text", sHeadText.substr(0, nLen)+ "▲");
				objDs.set_keystring("S:+" + S.SORT);
			   }
			   else 
			   {
				obj.setCellProperty( "head", i, "text", sHeadText+"▲");
				objDs.set_keystring("S:+" + S.SORT);
			   }
		  }
		  else
		  {
		   if (sHeadText.substr(nLen) == "▲" || sHeadText.substr(nLen) == "▼") 
		   {
			obj.setCellProperty( "head", i, "text", sHeadText.substr(0, nLen));
		   }
		  }
	 } 
