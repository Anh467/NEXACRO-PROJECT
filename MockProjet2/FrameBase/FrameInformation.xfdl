		S.SORT= S.SORT_NAME
		else S.SORT= sColId[1]
		
		if(!((S.SORT==S.SORT_NAME) || (S.SORT==S.SORT_DATE) ))  
			return;
		if (i == e.cell )
		  {
			   if (sHeadText.substr(nLen) == "▲") 
