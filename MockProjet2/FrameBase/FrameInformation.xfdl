		var nLen      = sHeadText.length - 1;  
		var S={
			SORT: "",
			SORT_DATE:"DATE",
			SORT_IMAGE: "IMAGE",
			SORT_NAME: "NAME"
		   }
	    var sColId = (obj.getCellProperty("body", e.col,"text")).toString().split(":");
	    trace("check here: "+(sColId[1]== S.SORT_IMAGE))
